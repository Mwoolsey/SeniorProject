<p id="notice"><%= notice %></p>

<h1>Internal Transfers</h1>

<table>
  <thead>
    <tr>
      <th>Originacctnumber</th>
      <th>Destinationacctnumber</th>
      <th>Amount</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @internal_transfers.each do |internal_transfer| %>
      <tr>
        <td><%= internal_transfer.originAcctNumber %></td>
        <td><%= internal_transfer.destinationAcctNumber %></td>
        <td><%= internal_transfer.amount %></td>
        <td><%= internal_transfer.account %></td>
        <td><%= link_to 'Show', internal_transfer %></td>
        <td><%= link_to 'Edit', edit_internal_transfer_path(internal_transfer) %></td>
        <td><%= link_to 'Destroy', internal_transfer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Internal Transfer', new_internal_transfer_path %>
