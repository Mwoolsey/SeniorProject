<p id="notice"><%= notice %></p>

<h1>Outgoing Transfers</h1>

<table>
  <thead>
    <tr>
      <th>Originacctnumber</th>
      <th>Destinationacctnumber</th>
      <th>Routingnumber</th>
      <th>Amount</th>
      <th>Account</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outgoing_transfers.each do |outgoing_transfer| %>
      <tr>
        <td><%= outgoing_transfer.originAcctNumber %></td>
        <td><%= outgoing_transfer.destinationAcctNumber %></td>
        <td><%= outgoing_transfer.routingNumber %></td>
        <td><%= outgoing_transfer.amount %></td>
        <td><%= outgoing_transfer.account %></td>
        <td><%= link_to 'Show', outgoing_transfer %></td>
        <td><%= link_to 'Edit', edit_outgoing_transfer_path(outgoing_transfer) %></td>
        <td><%= link_to 'Destroy', outgoing_transfer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Outgoing Transfer', new_outgoing_transfer_path %>
